  <SQLQuery>
    <![CDATA[SELECT * FROM IEDOC_IMP_BBLC_LINES L
WHERE 1=1
 AND  L.IMP_BBLC_LINE_ID =
                    (SELECT MAX (IIL.IMP_BBLC_LINE_ID)
                       FROM IEDOC_IMP_BBLC_LINES IIL
                      WHERE IIL.IMP_BBLC_HEADER_ID = L.IMP_BBLC_HEADER_ID
                        AND IIL.BBLC_NO = L.BBLC_NO)]]>
