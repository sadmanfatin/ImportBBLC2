  <SQLQuery>
    <![CDATA[SELECT H.IMP_SALE_CONTRACT_NO, 
       H.CONTRACT_DATE, 
       H.EXPORT_LC_NO, 
       H.ORG_ID,
       H.GOODS_VALUE,
       H.CONTRACT_VALUE, 
       H.CONTRACT_QUANTITY,
       H.VERSION_NO ,           
       H.SEASON,
       H.BUYER_ID  ,
       (SELECT AC.CUSTOMER_NAME from AR_CUSTOMERS AC WHERE AC.CUSTOMER_ID  = H.BUYER_ID AND AC.STATUS = 'A' ) BUYER_NAME_NEW
                  
  FROM XX_COM_SALES_CONTRACT_H_T H
  WHERE NOT EXISTS  (SELECT 1 FROM IEDOC_IMP_BBLC_CONTRACTS BBLCContractEO WHERE BBLCContractEO.BBLC_CONTRACT_NO=H.IMP_SALE_CONTRACT_NO
      and   nvl ( BBLCContractEO.ATTRIBUTE10 , 0)  = nvl(  H.VERSION_NO , 0 )
  )
  ORDER BY IMP_SALE_CONTRACT_NO , VERSION_NO DESC]]>
  </SQLQuery>
