  <SQLQuery>
    <![CDATA[SELECT BBLCHeaderEO.IMP_BBLC_HEADER_ID, 
       BBLCHeaderEO.PROPOSAL_NUMBER, 
       BBLCHeaderEO.PROPOSAL_DATE, 
       BBLCHeaderEO.IMPORT_AGAINST_CONTRACT, 
       BBLCHeaderEO.NEGOTIATION_BANK, 
       BBLCHeaderEO.CONTRACT_VALUE, 
       BBLCHeaderEO.LAST_SHIP_DATE, 
       BBLCHeaderEO.ENTITLE_BTB_VALUE, 
       BBLCHeaderEO.AVAILABLE_BTB_VALUE, 
       BBLCHeaderEO.VERSION_NO, 
       BBLCHeaderEO.VERSION_DATE, 
       BBLCHeaderEO.ORG_ID, 
       BBLCHeaderEO.LAST_UPDATE_LOGIN, 
       BBLCHeaderEO.LAST_UPDATE_DATE, 
       BBLCHeaderEO.LAST_UPDATED_BY, 
       BBLCHeaderEO.CREATION_DATE, 
       BBLCHeaderEO.CREATED_BY, 
       BBLCHeaderEO.ATTRIBUTE1, 
       BBLCHeaderEO.ATTRIBUTE2, 
       BBLCHeaderEO.ATTRIBUTE3, 
       BBLCHeaderEO.ATTRIBUTE4, 
       BBLCHeaderEO.ATTRIBUTE5, 
       BBLCHeaderEO.ATTRIBUTE6, 
       BBLCHeaderEO.ATTRIBUTE7, 
       BBLCHeaderEO.ATTRIBUTE8, 
       BBLCHeaderEO.ATTRIBUTE9, 
       BBLCHeaderEO.ATTRIBUTE10, 
       BBLCHeaderEO.ATTRIBUTE11, 
       BBLCHeaderEO.ATTRIBUTE12, 
       BBLCHeaderEO.ATTRIBUTE13, 
       BBLCHeaderEO.ATTRIBUTE14, 
       BBLCHeaderEO.ATTRIBUTE15, 
       BBLCHeaderEO.ATTRIBUTE16, 
       BBLCHeaderEO.ATTRIBUTE17, 
       BBLCHeaderEO.ATTRIBUTE18, 
       BBLCHeaderEO.ATTRIBUTE19, 
       BBLCHeaderEO.ATTRIBUTE20, 
       BBLCHeaderEO.ATTRIBUTE21, 
       BBLCHeaderEO.ATTRIBUTE22, 
       BBLCHeaderEO.ATTRIBUTE23, 
       BBLCHeaderEO.ATTRIBUTE24, 
       BBLCHeaderEO.ATTRIBUTE25, 
       BBLCHeaderEO.ATTRIBUTE26, 
       BBLCHeaderEO.ATTRIBUTE27, 
       BBLCHeaderEO.ATTRIBUTE28, 
       BBLCHeaderEO.ATTRIBUTE29, 
       (SELECT HOU.INTERNAL_ADDRESS_LINE
        FROM HR_ORGANIZATION_UNITS HOU 
        WHERE HOU.ORGANIZATION_ID = BBLCHeaderEO.ORG_ID AND ROWNUM = 1) ORG_NAME,
        
          (SELECT   substr(HOU.name , 0 , instr(HOU.name, ' ' )-1 )  
        FROM HR_ORGANIZATION_UNITS HOU 
        WHERE HOU.ORGANIZATION_ID = BBLCHeaderEO.ORG_ID AND ROWNUM = 1) ORG_SHORT_NAME,
        
        
        
       BBLCHeaderEO.ATTRIBUTE30,
       BBLCHeaderEO.BBLC_DOC_NUMBER,
        BBLCHeaderEO.PAID_AMOUNT_FOR_PROPOSAL ,
       (SELECT SUBSTR(HOU.NAME,0,INSTR(HOU.NAME,' '))||' / '||TO_CHAR(SYSDATE,'YYYY')||' / '||BBLCHeaderEO.BBLC_DOC_NUMBER
        FROM HR_ORGANIZATION_UNITS HOU 
        WHERE HOU.ORGANIZATION_ID = BBLCHeaderEO.ORG_ID AND ROWNUM = 1) PROPOSAL_NO_NEW,
BBLCHeaderEO.NEGOTIATION_BANK_ID,
(SELECT FU.USER_NAME FROM FND_USER FU WHERE FU.USER_ID = BBLCHeaderEO.CREATED_BY) CREATED_BY_NAME ,

--
--(SELECT SUM(CONTRACT_VALUE) FROM
--IEDOC_IMP_BBLC_CONTRACTS CC
--WHERE CC.IMP_BBLC_HEADER_ID=BBLCHEADEREO.IMP_BBLC_HEADER_ID) CONTRACT_VAL   ,

 
--  CONTRACT_VAL modified on 18 april 18 to handle version ; 
--  nvl ( (  nullif ( (SELECT SUM(CONTRACT_VALUE) FROM
--    IEDOC_IMP_BBLC_CONTRACTS CC
--    WHERE CC.IMP_BBLC_HEADER_ID=BBLCHEADEREO.IMP_BBLC_HEADER_ID
--    AND   CC.ATTRIBUTE10  =  (
--                                  select  max( attribute10) from IEDOC_IMP_BBLC_CONTRACTS CC1 
--                                  where cc1.IMP_BBLC_HEADER_ID = cc.IMP_BBLC_HEADER_ID
--                                  and CC1.BBLC_CONTRACT_NO = CC.BBLC_CONTRACT_NO
--                                            
--                                )),0)  
--            )  , 1) CONTRACT_VAL ,
--            
--            
--             nvl((select sum(l.TOTAL_BBLC_VALUE) 
--             from iedoc_imp_bblc_lines l 
--             where l.imp_bblc_header_id =BBLCHeaderEO.imp_bblc_header_id  
--             and l.imp_bblc_line_id =  (select max(l2.imp_bblc_line_id )from iedoc_imp_bblc_lines l2 where l2.bblc_no  = l.bblc_no )
--             
--               ), 0) TOTAL_BBLC_VALUE  ,
--               
               BBLCHeaderEO.CREATED_BY_ORIGINAL ,
               BBLCHeaderEO.CREATED_BY_CHANGE_DATE , 
               BBLCHeaderEO.TT_FDD_VALUE , 
                
                (SELECT LISTAGG ( BC.BBLC_CONTRACT_NO, ', ' )   WITHIN GROUP( ORDER BY BC.BBLC_CONTRACT_NO )FROM              
               (  SELECT DISTINCT  BC.BBLC_CONTRACT_NO FROM 
                IEDOC_IMP_BBLC_CONTRACTS BC WHERE  BC.IMP_BBLC_HEADER_ID =BBLCHEADEREO.IMP_BBLC_HEADER_ID) BC
               ) CONTRACT_NUMBERS,
               
               
                 BBLCHeaderEO.SHIPPING_GURANTEE_NUMBER   ,
                 
                 BBLCHeaderEO.TOTAL_CONTRACT_LIABILITY ,
                 BBLCHeaderEO.TOTAL_COMMISSION_VALUE,
                  BBLCHeaderEO.TOTAL_GOODS_VALUE,
                  BBLCHeaderEO.PROPOSAL_OPEN_STATUS
                   
FROM IEDOC_IMP_BBLC_HEADERS BBLCHeaderEO
Where 1=1
ORDER BY BBLCHeaderEO.BBLC_DOC_NUMBER DESC]]>
  </SQLQuery>
