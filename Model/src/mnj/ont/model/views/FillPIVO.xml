  <SQLQuery>
    <![CDATA[SELECT DISTINCT PIL.PI_NO, 
       PIL.PI_DATE PI_DATE, 
       PIL.ORG_ID,
       PIL.TOTAL_VALUE PI_VALUE,
         
       IIPA.FREIGHT_CHARGES,
       IIPA.BANK_CHARGES, 
       IIPA.HANDLING_CHARGES, 
       IIPA.DISCOUNT,
       IIPA.OTHER,
       'BPO NUMBER' BPO_NUMBER,
       'STYLE NUMBER' STYLE_NUMBER,
       PIL.CURRENCY,
       PID.ATTRIBUTE3 BUYER_NAME,
       PID.ATTRIBUTE4 SEASON ,
       PIL.supplier_id ,
       (select vendor_name  from ap_suppliers ap
       where ap.vendor_id=SUPPLIER_ID) Supplier_Name
       
FROM IEDOC_IMP_PI_LINES PIL , IEDOC_IMP_PI_DETAILS PID, IEDOC_IMP_PI_ADJUSTMENTS IIPA
WHERE PIL.IMP_PI_LINE_ID = IIPA.IMP_PI_LINE_ID(+)
AND PIL.IMP_PI_LINE_ID = PID.IMP_PI_LINE_ID 
AND PID.IMP_PI_DETAIL_ID = (SELECT MAX (IMP_PI_DETAIL_ID ) FROM IEDOC_IMP_PI_DETAILS WHERE  PIL.IMP_PI_LINE_ID =IEDOC_IMP_PI_DETAILS.IMP_PI_LINE_ID  )

AND NOT EXISTS (SELECT 'X' FROM IEDOC_IMP_BBLC_PI_DETAILS PI
                  WHERE PI.PI_NO=PIL.PI_NO 
                  and PIL.supplier_id =  pi.BENEFICIARY_ID
             --  and  PI.IMP_BBLC_LINE_ID = :P_BBLC_LINE_ID
                 ) 

ORDER BY PIL.PI_NO]]>
  </SQLQuery>
