  Name="InsuranceCompanyNameLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.InsuranceCompanyNameLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT CHARACTER5 ISURANCE_COMPANY,CHARACTER1 INSURANCE_COVER_NOTE_NO,
       TO_DATE (CHARACTER2, 'YYYY-MM-DD') INSURANCE_COVER_DATE,CHARACTER4 ORGANIZATION_NAME
  FROM QA_RESULTS QR
 WHERE QR.PLAN_ID =
          (SELECT PLAN_ID
             FROM QA_PLANS
            WHERE NAME = 'INS COVER NOTE NO DETAIL' AND ORGANIZATION_ID = 354)   
           
            
                ORDER BY CHARACTER1]]>
  </SQLQuery>
