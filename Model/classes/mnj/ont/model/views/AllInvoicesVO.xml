  <SQLQuery>
    <![CDATA[SELECT  H.IMP_BBLC_HEADER_ID, H.PROPOSAL_NUMBER , L.BBLC_NO , L.ATTRIBUTE15  LC_STATUS ,
 D.INVOICE_NO , D.INVOICE_DATE , D.SHIPMENT_STATUS,D.SHIPMENT_MODE , D.IMPORT_ITEM, D.IMPORT_VALUE , D.FREIGHT_COST , D.NO_OF_PACKAGES , D.ETD_ORIGION, D.ATD_ORIGION, D.ETA_HUB_PORT , D.ATA_HUB_PORT , D.ETD_HUB_PORT , D.ATD_HUB_PORT,
 D.ETA_DESTINATION , D.ATA_DESTINATION , D.BERTHING_DATE , NN_COPY_RCVD_DATE ,D.PORT_OF_LOADING, D.PORT_OF_DISCHARGE , D.M_VESSEL_VOYAGE , D.CONTAINER_NO ,D.CONTAINER_STATUS , D.CONSIGNMENT_DATE , D.ENDORSEMENT , D.ENDORSEMENT_DATE,
 D.DOX_TO_CFAGENT_DATE , D.CF_AGENT_NAME  ,D.COURIER_NAME_BB_DOX,D.DISPATCH_AWB_NO,D.LETTER_SHIP_ISUE_DATE,D.ORIGINAL_BL_COLLECT_BANK,D.BL_TO_CNF_COLLECT_DATE ,D.SG_TEE_COLLECT_CNF_DATE,D.SG_TEE_SUBMIT_BANK,CO_RCPT_HO_CONCERN_DATE   ,
D.IN_HOUSE_DATE   ,
A.BILL_REFERENCE_NO , A.BILL_REFERENCE_DATE , A.BILL_VALUE , A.ACCEPTANCE_DATE , A.MATURITY_DATE , A.CURRENCY , A.PAYMENT_NO, A.PAYMENT_AMOUNT
FROM  
IEDOC_IMP_BBLC_HEADERS H,
IEDOC_IMP_BBLC_LINES L,
IEDOC_IMP_BBLC_DETAILS D ,

(SELECT   AA.BILL_REFERENCE_NO , AA.BILL_REFERENCE_DATE , AA.BILL_VALUE , AA.ACCEPTANCE_DATE , AA.MATURITY_DATE , AA.CURRENCY , AA.PAYMENT_NO, AA.PAYMENT_AMOUNT , AD.INVOICE_NO
FROM IEDOC_IMP_BBLC_ACCEPTANCE AA ,
IEDOC_IMP_BBLC_ACCEPT_INV_DET AD 
WHERE AA.IMP_BBLC_ACCEPTANCE_ID = AD.IMP_BBLC_ACCEPTANCE_ID  

) A

WHERE 1=1
AND H.IMP_BBLC_HEADER_ID =L.IMP_BBLC_HEADER_ID
AND L.IMP_BBLC_LINE_ID = D.IMP_BBLC_LINE_ID 
AND D.INVOICE_NO  = A.INVOICE_NO(+)

ORDER BY  L.BBLC_NO , L.IMP_BBLC_LINE_ID ASC]]>
  </SQLQuery>
