       BBLCLinesEO.USANCE_PERIOD, 
       BBLCLinesEO.SHIPMENT_MODE, 
       BBLCLinesEO.BENEFICIARY_NAME, 
       BBLCLinesEO.PI_NO, 
       BBLCLinesEO.TOTAL_BBLC_VALUE, 
       BBLCLinesEO.ITEM, 
       BBLCLinesEO.LCAF_NO, 
       BBLCLinesEO.COUNTRY_OF_ORIGION, 
       BBLCLinesEO.CONFIRMATION, 
       BBLCLinesEO.CONFIRAMTION_CHARGES, 
       BBLCLinesEO.FREIGHT_CHARGE, 
       BBLCLinesEO.OTHER_CHARGE, 
       BBLCLinesEO.COMMISSION, 
       BBLCLinesEO.INSURANCE_COMPANY_NAME, 
       BBLCLinesEO.INSURANCE_POLICY_NO, 
       BBLCLinesEO.UD_AMENDMENT_NO, 
       BBLCLinesEO.LC_STATUS, 
       BBLCLinesEO.LAST_UPDATE_LOGIN, 
       BBLCLinesEO.LAST_UPDATE_DATE, 
       BBLCLinesEO.LAST_UPDATED_BY, 
       BBLCLinesEO.CREATION_DATE, 
       BBLCLinesEO.CREATED_BY, 
       BBLCLinesEO.ATTRIBUTE1, 
       BBLCLinesEO.ATTRIBUTE2, 
       BBLCLinesEO.ATTRIBUTE3, 
       BBLCLinesEO.ATTRIBUTE4, 
       BBLCLinesEO.ATTRIBUTE5, 
       BBLCLinesEO.ATTRIBUTE6, 
       BBLCLinesEO.ATTRIBUTE7, 
       BBLCLinesEO.ATTRIBUTE8, 
       BBLCLinesEO.ATTRIBUTE9, 
       BBLCLinesEO.ATTRIBUTE10, 
       BBLCLinesEO.ATTRIBUTE11, 
       BBLCLinesEO.ATTRIBUTE12, 
       BBLCLinesEO.ATTRIBUTE13, 
       BBLCLinesEO.ATTRIBUTE14, 
       BBLCLinesEO.ATTRIBUTE15, 
       BBLCLinesEO.ATTRIBUTE16, 
       BBLCLinesEO.ATTRIBUTE17, 
       BBLCLinesEO.ATTRIBUTE18, 
       BBLCLinesEO.ATTRIBUTE19, 
       BBLCLinesEO.ATTRIBUTE20, 
       BBLCLinesEO.ATTRIBUTE21, 
       BBLCLinesEO.ATTRIBUTE22, 
       BBLCLinesEO.ATTRIBUTE23, 
       BBLCLinesEO.ATTRIBUTE24, 
       BBLCLinesEO.ATTRIBUTE25, 
       BBLCLinesEO.ATTRIBUTE26, 
       BBLCLinesEO.ATTRIBUTE27, 
       BBLCLinesEO.ATTRIBUTE28, 
       BBLCLinesEO.ATTRIBUTE29, 
       BBLCLinesEO.ATTRIBUTE30, 
       BBLCLinesEO.COMMUNICATION_TYPE, 
       BBLCLinesEO.UPASS_DAYS, 
       BBLCLinesEO.INSURANCE_COVER_NOTE_DATE, 
       BBLCLinesEO.CURRENCY, 
       BBLCLinesEO.CONVERSION_RATE, 
       BBLCLinesEO.CONVERTED_AMOUNT,
       (
SELECT EXPORT_LC_NUMBER||' / '||IIUH.ATTRIBUTE3
  FROM IEDOC_IMP_UD_DETAIL_CONSUM IIUDC,
       IEDOC_IMP_UD_LINES_CONTRACT IIULC,
       IEDOC_IMP_UD_HEADERS IIUH
 WHERE IIUDC.IMP_UD_LINE_CONTRACT_ID = IIULC.IMP_UD_LINE_CONTRACT_ID
   AND IIULC.IMP_UD_HEADER_ID = IIUH.IMP_UD_HEADER_ID
   AND BBLC_NO = BBLCLinesEO.BBLC_NO
   AND BBLCLinesEO.ATTRIBUTE15 = IIUDC.ATTRIBUTE1 
   AND ROWNUM = 1
       ) UD_AMENDMENT_NUMBER, 
       BBLCLinesEO.AMENDMENT_DATE,
(select iibpd.ATTRIBUTE1 USD from 
IEDOC_IMP_PI_LINES PIL ,ap_suppliers ap,IEDOC_IMP_BBLC_PI_DETAILS iibpd
where ap.VENDOR_ID=pil.SUPPLIER_ID
and iibpd.PI_NO=pil.PI_NO
and iibpd.IMP_BBLC_LINE_ID=BBLCLinesEO.IMP_BBLC_LINE_ID
and rownum=1) Curr,

BBLCLinesEO.BENEFICIARY_ID,

BBLCLinesEO.AMENDED_VALUE ,
BBLCLinesEO.PAID_AMOUNT_FOR_BBLC 
FROM IEDOC_IMP_BBLC_LINES BBLCLinesEO
ORDER BY BBLCLinesEO.IMP_BBLC_LINE_ID ASC
/*Order by BBLC_NO,Decode(attribute15,'Original',0,Substr(attribute15,7))*/]]>
  </SQLQuery>
